TOKENS {
    INT: "int",
    VOID: "void",
    A: "asd",
    B: "basd",
    ID: "[[:alpha:]][[:alnum:]_]*"
}

SKIP {
    " ",
    "\n",
    "\t"
}

Start -> (First)* { "test" : "first" } Second;

First -> < INT > { "hello" : last_token } ;
First -> < VOID >;
First -> Third Fourth;

Third -> < A >;
Fourth -> < B >;

Second -> (< ID >)+;